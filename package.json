{
  "name": "@auctelia/kosa",
  "version": "1.1.6",
  "description": "Custom error class to help handle errors at different level with aligned behaviors",
  "type": "module",
  "directories": {
    "test": "test",
    "dist": "dist"
  },
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs",
    "test": "LOG_LEVEL=fatal mocha"
  },
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./index.d.ts"
  },
  "files": [
    "./dist/index.mjs",
    "./dist/index.cjs",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auctelia/kosa.git"
  },
  "keywords": [
    "error",
    "errors"
  ],
  "author": "@auctelia",
  "contributors": [
    "Olivier Cuypers <oliviercuyp@gmail.com>",
    "Louis Borsu <sat@satprod.net>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/auctelia/kosa/issues"
  },
  "homepage": "https://github.com/auctelia/kosa#readme",
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "before",
      "after"
    ],
    "ignore": "dist"
  },
  "devDependencies": {
    "chai": "4.0.0",
    "mocha": "11.5.0",
    "mocha-standard": "1.0.0",
    "rollup": "4.24.0",
    "semistandard": "17.0.0",
    "snazzy": "7.0.0"
  },
  "dependencies": {
    "inherits-ex": "2.1.0"
  }
}
